/* pages/chooseType/chooseType.wxss */
page {
  background-color: #eee;
}
.item-contain{
  padding-bottom: 100rpx;
}
.title{
  color: #737373;
  font-size: 26rpx;
  padding: 20rpx 30rpx;
}
.item {
  background-color: #fff;
  color: #333;
  font-size: 30rpx;
  padding: 20rpx 30rpx;
  margin-bottom: 10rpx;
}
.check-img{
  width: 40rpx;
  height: 40rpx;
}
.button {
  position: fixed;
  width: 100%;
  height: 100rpx;
  bottom: 0;
  background: linear-gradient(to right,rgb(19, 191, 222), rgb(120, 233, 250));
  color: #fff;
  font-size: 34rpx;
  line-height: 100rpx;
  margin: 0 auto;
  text-align: center;
}