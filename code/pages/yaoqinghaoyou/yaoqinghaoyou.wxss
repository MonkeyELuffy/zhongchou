/* pages/yaoqinghaoyou/yaoqinghaoyou.wxss */
page{
  background-color: #eee;
}
.interview{
  background-color: #fff;
  font-size: 30rpx;
  color: #333;
  padding: 30rpx;
  align-items: center;
}
/*横向排列的菜单*/
.nav-title {
  background-color: #fff;
  color: #666;
  font-size: 30rpx;
  white-space: nowrap; 
  box-shadow: 0rpx 0rpx 2rpx #ddd ;
  padding: 10rpx 40rpx;
  box-sizing:border-box;
  border-top: 10rpx solid #eee;
}
.nav-item {
  display: inline-block;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
}
.item{
  padding: 0 20rpx;
  background-color: #eee;
  margin-bottom: 8rpx;
}
.img-contain{
  padding: 20rpx 0;
  margin-bottom: 2rpx;
  align-items: center;
  background-color: #fff;
}
.item-image{
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  margin: 0 30rpx;
}
.item-name{
  font-size: 26rpx;
  color: #333;
}
.detail{
  font-size: 22rpx;
  color: #999;
  padding: 30rpx;
  background-color: #fff;
}
.btn{
  background: linear-gradient(to left,rgb(19, 191, 222), rgb(120, 233, 250));
  color: #fff;
  font-size: 26rpx;
  text-align: center;
  padding: 0;
  width: 120rpx;
  border-radius: 10rpx;
  margin: 0 20rpx;
}
.sn{
  margin-right: 80rpx;
}