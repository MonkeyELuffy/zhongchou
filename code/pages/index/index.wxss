.contain{
  background-color: #eee;
}
.swiper{
  height: 330rpx;
}

/*banner  */
.swiper-container{
  width: 100%;
}
.top-nav{
  width: 100%;
  position: absolute;
  z-index: 10;
  display: flex;
  padding: 30rpx;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
/*扫描  */
.saomiao{
  width: 56rpx;
  height: 56rpx;
}
/*搜索框*/
.search {
  flex: 1 1;
  z-index: 100;
  background-color: transparent;
  position: relative;
  box-sizing: border-box;
  padding: 0 20rpx;
}
.search input {
  flex: 1 1;
  height: 60rpx;
  background-color: #fff;
  color: #333;
  font-size: 24rpx;
  padding-left: 80rpx;
  border-radius: 30rpx;
}
.search .icon {
  position: absolute;
  left: 40rpx;
  top: 14rpx;
  width: 30rpx;
  height: 30rpx;
}
/*定位  */
.position{
  border-radius: 50rpx;
  background-color: rgba(217, 217, 217, 0.5);
  color: red;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 10rpx;
  height: 60rpx;
  box-sizing: border-box;
}
.position-image{
  width: 26rpx;
  height: 32rpx;
  margin-right: 10rpx;
}

/*八个小菜单*/
.nav3-list {
  background-color: #fff;
  position: relative;
  display: flex;
  flex-wrap: wrap;
  border-bottom: 10rpx solid #eee;
  padding-top: 40rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  box-shadow: 0 8rpx 10rpx #ccc;
  margin: 20rpx;
  justify-content: space-between;
}
.nav3-list-image {
  border-radius: 50%;
  overflow: hidden;
  display: flex;
  justify-content: center;
  align-items: center;
}
.nav3-list .nav3-item {
  width: 24%;
  display: flex;
  padding-bottom: 54rpx;
  flex-direction: column;
  justify-content: center;
  align-items: center;

}
.nav3-list .nav3-item image {
 width: 100rpx;
 height: 100rpx; 
}
.nav3-list .nav3-item .nav3-name {
  font-size: 24rpx;
  padding-top: 6rpx;
  color: #666;
}

/*排序  */
.tab{
  display: flex;
  align-items: center;
  background-color: #fff;
  border-bottom: 10rpx solid #ddd;
  border-top: 20rpx solid #ddd;
  color: #333;
}
.tab-item{
  width: 33%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  padding: 30rpx 0;
}
.tab-up-down{
  width: 34rpx;
  height: 50rpx;
  margin-left: 20rpx;
}

/*数据列表  */
.data-list{
  width: 100%;
  position: absolute;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
}
.item{
  padding:20rpx 0 0 20rpx;
}
.item-image{
  width: 184rpx;
  height: 184rpx;
  box-sizing: border-box;
  margin-right: 30rpx;
}
.detail-name{
  font-size: 32rpx;
  color: #333;
  font-weight: 700;
  padding-bottom: 4rpx;
}
.label-contain{
  font-size: 24rpx;
}
.label-item{
  padding: 5rpx 10rpx;
  border: 1rpx solid #f68076;
  margin-right: 8rpx;
  min-width: 76rpx;
  text-align: center;
}
.haoping{
  font-size: 28rpx;
  color: #666;
  padding-left: 10rpx;
}
.sale-and-juli{
  margin-top: 40rpx;
  color: #b2b2b2;
  font-size: 28rpx;
  border-bottom: 1rpx solid #b2b2b2;
  padding-bottom: 30rpx;
}
.juli{
  padding-right: 20rpx;
}

/*客服  */
.kefu,.ruzhu{
  position: fixed;
  right: 40rpx;
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 50%;
  color: #fff;
  font-weight: 700;
  font-size: 32rpx;
  text-align: center;
  box-sizing: border-box;
}
.kefu{
  bottom: 40rpx;
  background:linear-gradient(to right,rgb(19, 191, 222), rgb(120, 233, 250));
}
.ruzhu{
  bottom: 160rpx;
  background:linear-gradient(to right,rgb(255, 141, 151), rgb(250, 180, 180));
}

.nav2{
  padding: 20rpx;
  overflow: hidden;
  border-radius: 10rpx;
  color: #fff;
}
.nav2-item{
  height: 194rpx;
  margin-bottom: 10rpx;
}
.nav2-left{
  font-size: 32rpx;
  height: 194rpx;
  width: 244rpx;
  position: relative;
  margin-right: 8rpx;
  padding: 30rpx 0 0 30rpx;
  box-sizing: border-box;
}
.nav2-img{
  width: 130rpx;
  height: 100rpx;
  position: absolute;
  z-index: 100;
  right: 40rpx;
  bottom: 20rpx;
}
.nav2-right{
  flex-wrap: wrap;
}
.nav2-right-item{
  font-size: 30rpx;
  width: 49.5%;
  height: 96rpx;
  line-height: 96rpx;
  text-align: center;
  display: inline-block;
  margin: 2rpx 0;
}